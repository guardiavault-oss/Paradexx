    "node": ">=20.0.0",
    "pnpm": ">=10.16.1"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "cross-env NODE_ENV=production vite build && cross-env NODE_ENV=production tsx scripts/build-server.ts",
    "build:client": "cross-env NODE_ENV=production vite build",
    "build:server": "cross-env NODE_ENV=production tsx scripts/build-server.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "check:deployment": "tsx scripts/check-deployment-ready.ts",
    "verify:build": "node scripts/verify-build.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx server/scripts/migrate.ts up",
    "db:migrate:down": "tsx server/scripts/migrate.ts down",
    "db:migrate:status": "tsx server/scripts/migrate.ts status",
    "db:migrate:death-verification": "tsx server/scripts/apply-death-verification-migration.ts",
    "db:migrate:landing-features": "tsx server/scripts/apply-landing-features-migration.ts",
    "db:migrate:fragment-scheme": "tsx scripts/run-fragment-migration.ts",
    "db:migrate:notifications": "tsx server/scripts/apply-notifications-migration.ts",
    "test:recovery": "tsx scripts/test-recovery-endpoint.ts",
    "compile": "hardhat compile",
    "setup:sepolia": "tsx scripts/setup-sepolia.ts",
    "deploy:all:sepolia": "tsx scripts/deploy-all-sepolia.ts",
    "deploy:local": "npx hardhat ignition deploy ignition/modules/GuardiaVault.ts --network hardhat",
    "deploy:sepolia": "npx hardhat ignition deploy ignition/modules/GuardiaVault.ts --network sepolia",
    "deploy:yield:local": "npx hardhat ignition deploy ignition/modules/YieldVault.ts --network hardhat",
    "deploy:yield:sepolia": "npx hardhat ignition deploy ignition/modules/YieldVault.ts --network sepolia",
    "deploy:yield": "tsx scripts/deploy-yield-vault.ts",
    "test": "npm run test:backend && npm run test:frontend",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:contracts",
    "test:unit": "npm run test:backend && npm run test:frontend",
    "test:contracts": "npx hardhat test",
    "test:backend": "vitest run --config vitest.config.ts",
    "test:frontend": "vitest run --config vitest.config.client.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "ui:enhance": "node scripts/ui-enhancer.cjs",
    "test:e2e:debug": "playwright test --debug",
    "test:mobile": "cd mobile && npm test",
    "test:mobile:watch": "cd mobile && npm run test:watch",
    "test:mobile:coverage": "cd mobile && npm test -- --coverage",
    "test:watch": "vitest --config vitest.config.ts",
    "test:watch:frontend": "vitest --config vitest.config.client.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts && vitest run --coverage --config vitest.config.client.ts",
    "test:coverage:backend": "vitest run --coverage --config vitest.config.ts",
    "test:coverage:frontend": "vitest run --coverage --config vitest.config.client.ts",
    "test:mutation": "stryker run",
    "test:visual": "percy exec -- playwright test",
    "test:load": "npm run test:load:artillery && npm run test:load:k6",
    "test:load:artillery": "artillery run tests/load/artillery/scenarios/basic-load.yml",
    "test:load:artillery:report": "artillery run tests/load/artillery/scenarios/basic-load.yml --output tests/load/artillery/reports/report.json && artillery report tests/load/artillery/reports/report.json",
    "test:load:k6": "k6 run tests/load/k6/scenarios/spike-test.js",
    "test:load:k6:cloud": "k6 cloud tests/load/k6/scenarios/spike-test.js",
    "test:chaos": "node tests/chaos/runner.js",
    "test:security": "npm run test:security:zap && npm run test:security:deps",
    "test:security:zap": "echo 'Run OWASP ZAP scan manually - see tests/security/README.md'",
    "test:security:deps": "npm audit --audit-level=moderate",
    "test:security:penetration": "node tests/security/penetration/runner.js",
    "test:performance": "lighthouse http://localhost:5000 --output=html --output-path=tests/performance/reports/lighthouse-report.html",
    "test:a11y": "pa11y-ci --config tests/accessibility/.pa11yci.json",
    "test:smoke": "vitest run --config tests/smoke/vitest.config.smoke.ts",
    "test:contract": "vitest run --config tests/contract/vitest.config.contract.ts",
    "node:local": "npx hardhat node",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test:env-validation": "tsx scripts/test-env-validation.ts",
    "test:compression": "tsx scripts/test-compression.ts",
    "optimize:images": "tsx scripts/optimize-images.ts",
    "build:analyze": "pnpm run build && echo 'Bundle analysis available at dist/public/stats.html'",
    "audit:security": "tsx scripts/security-audit.ts",
    "audit:edge-cases": "tsx scripts/edge-case-audit.ts",
    "audit:all": "npm run audit:security && npm run audit:edge-cases",
    "verify:security": "tsx scripts/verify-security-fixes.ts",
    "deploy:check": "tsx scripts/final-deployment-check.ts",
    "validate:checklist": "tsx scripts/validate-production-checklist.ts",
    "dev:dashboard": "concurrently \"npm run dev\" \"cd client && npm run dev\"",
    "dashboard": "cd client && npm run dev"
  },
