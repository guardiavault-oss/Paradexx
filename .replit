modules = ["nodejs-20", "python-3.11", "web"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0", "resend:1.0.0", "stripe:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ScarletteAI"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backend API"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MEV Protection"

[[workflows.workflow]]
name = "ScarletteAI"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd scarlette-ai-service-standalone && python -m uvicorn src.scarlette_ai.main:app --host 0.0.0.0 --port 8000"
waitForPort = 8000

[[workflows.workflow]]
name = "Backend API"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd src/backend && npx tsx watch server.ts"
waitForPort = 3001

[[workflows.workflow]]
name = "Frontend"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pnpm vite --host 0.0.0.0 --port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "MEV Protection"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd Mev_Protection && pip install -q fastapi uvicorn pydantic && uvicorn api:app --host 0.0.0.0 --port 8080"
waitForPort = 8080

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 41441
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

